        <div class="container body-content">
            @RenderBody()
        </div>


        <footer class="esh-app-footer">
            <div class="container">
                <article class="row">
                    <section class="offset-sm-2 col-sm-4">
                        <div class="text-center footer-img">
                            <img width="319px" src="~/images/flor-com-logo-1.png" />
                        </div>
                        <div class="esh-app-footer-text footer-text"> © Copyright 2017 Dama no Jornal | All Rights Reserved </div>
                    </section>
                    <section class="col-12 col-sm-4 footer-links text-center">
                        <div class="footer-link text-center">
                            <a href="#">FAQ</a>
                        </div>
                        <div class="footer-link text-center">
                            <a href="#">Eventos</a>
                        </div>
                        <div class="footer-link text-center">
                            <a href="#">Contactos</a>
                        </div>
                        <div class="footer-link text-center text-nowrap">
                            <a href="#">Termos e Condições</a>
                        </div>
                        <div class="footer-link text-center">
                            <i class="fab fa-facebook-f fa-inverse"></i>
                            <i class="fab fa-instagram fa-inverse"></i>
                            <i class="fab fa-google-plus-g fa-inverse"></i>
                        </div>
                        <div class="esh-app-footer-text footer-text-mobile"> © Copyright 2017 Dama no Jornal | All Rights Reserved </div>
                    </section>
                </article>
            </div>
        </footer>

        <environment names="Development">
            <script src="~/lib/jquery/dist/jquery.js"></script>
            <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
            <script src="~/lib/fontawesome-free-5.0.4/js/fontawesome-all.js"></script>
            <script src="~/lib/slick-1.8.0/slick/slick.js"></script>
            <script src="~/js/site.js" asp-append-version="true"></script>
        </environment>
        <environment names="Staging,Production">
            <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"
                    asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                    asp-fallback-test="window.jQuery">
            </script>          
            <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"
