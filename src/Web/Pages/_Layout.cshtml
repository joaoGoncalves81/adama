<body>

    <nav class="navbar navbar-expand-md navbar-dark top-nav pt-0 pb-0 pl-5 pr-5 d-none d-sm-flex pt-sm-3">
        <a href="https://www.damanojornal.com" class="navbar-brand">
            <img src="~/images/brand_full.png" width="150" alt="Dama no Jornal" />
        </a>
        <form class="form-inline my-2 my-lg-0 search-text">
            <input class="bg-transparent border-left-0 border-right-0 border-top-0 border-light" autocomplete="off" type="search" placeholder="Procurar" aria-label="Search">
        </form>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar7">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="navbar-collapse collapse justify-content-stretch" id="navbar7">
            <ul class="navbar-nav ml-auto">
                @await Html.PartialAsync("_LoginPartial")
            </ul>
        </div>
    </nav>

    <nav class="navbar navbar-dark navbar-expand-md justify-content-between background_gradient pb-md-4 d-none d-sm-flex">
        <a href="/" class="pl-3 pl-lg-5 backToHome"><i class="fas fa-arrow-left"></i><span>Votar ao inicio</span></a>
        <div class="d-flex d-sm-none dama-login-mobile pl-5">
            @await Html.PartialAsync("_LoginPartial")
        </div>
        <div class="navbar-collapse collapse justify-content-center text-center">
            <div></div>
            @await Component.InvokeAsync("Menu")
        </div>
        <ul class="nav navbar-nav justify-content-end pr-5" style="width:188px">
            <li class="social-links"><a class="nav-link" href=""><i class="fab fa-facebook-f fa-inverse"></i></a></li>
            <li class="social-links"><a class="nav-link" href=""><i class="fab fa-instagram"></i></a></li>
            <li class="social-links"><a class="nav-link" href=""><i class="fab fa-google-plus-g"></i></a></li>
        </ul>
    </nav>
    <nav class="navbar navbar-dark navbar-expand-md justify-content-between background_gradient pb-md-4 d-flex d-sm-none">
        <a id="brand-mobile-image" class="navbar-brand" href="https://www.damanojornal.com">
            <img src="~/images/brand.png" width="50px;" alt="Dama no Jornal" />
        </a>
        <ul class="list-inline ml-auto">
            @await Html.PartialAsync("_LoginPartial")
        </ul>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#collapsingNavbar">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="navbar-collapse collapse text-center" id="collapsingNavbar">
            <div></div>
            @await Component.InvokeAsync("Menu")
        </div>
    </nav>


