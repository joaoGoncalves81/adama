@page
@model Web.Pages.Category.Type.IndexModel
@{
    ViewData["Title"] = "Index";
}

<h2>@Model.CatalogTypeName</h2>

<div class="news-heading text-center">
    <h2>Todos os Produtos</h2>
    <img class="separator" src="~/images/coração-rosa.png" />
</div>
<div class="container">

    <div class="esh-catalog-items row">
        @foreach (var catalogItem in Model.CatalogModel.CatalogItems)
        {
            <div class="esh-catalog-item col-md-3">
                @Html.Partial("_product", catalogItem)
            </div>
        }
    </div>
</div>
